[repl]
# Set to true to enable the remote REPL
enabled = <%= $::puppet::puppetdb::repl_enabled %>

# Either nrepl or swank or telnet
type = <%= $::puppet::puppetdb::repl_type %>

# What port the REPL should listen on
port = <%= $::puppet::puppetdb::repl_port %>

